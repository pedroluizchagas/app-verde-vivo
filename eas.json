{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote",
    "appDirectory": "mobile"
  },
  "build": {
    "development": {
      "ios": {
        "distribution": "internal",
        "developmentClient": true
      }
    },
    "preview": {
      "ios": {
        "distribution": "internal"
      },
      "android": {
        "distribution": "internal",
        "buildType": "apk",
        "credentialsSource": "local"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "distribution": "store"
      },
      "android": {
        "distribution": "store",
        "buildType": "app-bundle",
        "credentialsSource": "local"
      }
    }
  },
  "submit": {
    "ios": {},
    "android": {}
  }
}
